<dataConfig>
    <dataSource driver="org.postgresql.Driver" url="jdbc:postgresql://127.9.20.130:5432/fim" user="fim" password="fim" />
    <document>
        <entity name="user" query="select * from users_view"
                deltaQuery="select id from users_view where modified_at > '${dataimporter.last_index_time}'"
				deletedPkQuery="select id from fim_user where status = 'DISABLED'">

            <field name="firstName" 	column="first_column" />
            <field name="lastcolumn" 	column="last_column" />
            <field name="usercolumn" 	column="login_column" />
            <field name="defaultRole" 	column="default_role" />
            <field name="createdAt" 	column="created_at" />
            <field name="modifiedAt" 	column="modified_at" />

        </entity>
    </document>
</dataConfig>

